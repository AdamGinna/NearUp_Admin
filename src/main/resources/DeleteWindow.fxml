<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" xmlns="http://javafx.com/javafx/8.0.202-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.DeleteWindow">
   <children>
      <Label text="Are you sure you want to delete this?" textFill="RED">
         <VBox.margin>
            <Insets bottom="20.0" top="20.0" />
         </VBox.margin>
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <AnchorPane prefHeight="326.0" prefWidth="600.0">
         <children>
            <Label contentDisplay="RIGHT" layoutX="39.0" layoutY="164.0" text="longitude:">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label layoutX="47.0" layoutY="14.0" text="Id:">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label layoutX="47.0" layoutY="65.0" text="Name:">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label contentDisplay="RIGHT" layoutX="39.0" layoutY="115.0" text="latitude:">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="latitude" contentDisplay="RIGHT" layoutX="130.0" layoutY="115.0">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="name" contentDisplay="RIGHT" layoutX="125.0" layoutY="65.0">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="id" contentDisplay="RIGHT" layoutX="77.0" layoutY="14.0">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="longitude" contentDisplay="RIGHT" layoutX="146.0" layoutY="164.0">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="owner" contentDisplay="RIGHT" layoutX="108.0" layoutY="208.0">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label contentDisplay="RIGHT" layoutX="39.0" layoutY="208.0" text="Owner:">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <ImageView fx:id="image" fitHeight="208.0" fitWidth="305.0" layoutX="280.0" layoutY="93.0" pickOnBounds="true" preserveRatio="true" />
         </children>
      </AnchorPane>
      <HBox alignment="CENTER" prefHeight="71.0" prefWidth="600.0">
         <children>
            <Button mnemonicParsing="false" onMouseClicked="#delete" text="Delete" textFill="RED">
               <HBox.margin>
                  <Insets right="100.0" />
               </HBox.margin>
               <font>
                  <Font size="17.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onMouseClicked="#cancel" text="Cancel">
               <HBox.margin>
                  <Insets left="100.0" />
               </HBox.margin>
               <font>
                  <Font size="21.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
